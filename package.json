{
  "name": "john-website-react",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "animate.css": "^3.5.2",
    "bootstrap-styl": "^5.0.8",
    "devicons": "^1.8.0",
    "font-awesome-stylus": "^4.7.1-alpha.1",
    "nuka-carousel": "^4.2.1",
    "react": "^16.3.2",
    "react-bootstrap": "^0.31.1",
    "react-dom": "^16.3.2",
    "react-markdown": "^2.5.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "npm-run-all": "^4.0.2",
    "react-scripts": "1.1.4",
    "stylus": "^0.54.5"
  },
  "scripts": {
    "watch:css": "stylus -w -r src/ -o src/stylus.build.css",
    "watch:src": "react-scripts start",
    "start": "npm-run-all --parallel watch:css watch:src",
    "build:css": "stylus -r src/ -o src/stylus.build.css",
    "build:src": "react-scripts build",
    "build": "npm-run-all build:css build:src",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
